        <declaration>meta int dist = 0, dv;
            double r = 1.0;

            void set_r(int i_v) {
            int i, count=0;
            for (i=0;i&lt;N_PI;i++)
            if(PI_S[i] == PI_V[i_v][i]) count++;
            r = count/N_PI;
            }

            void set(double r, double s, bool update_time) {
            r_payoff = r;
            s_payoff = s;
            if (update_time) evac_time = t;
            else evac_time = 0;
            }
        </declaration>
