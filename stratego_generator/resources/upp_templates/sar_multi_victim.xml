    <system>r = Robot();

        // Rationality Degree: 1.0 -&gt; Perfectly Rational Agent, 0.0 -&gt; Perfectly Irrational Agent
        s = Survivor(0, 1, 5);

        system r,s;
    </system>
    <queries>
        <query>
            <formula>Pr[&lt;=TAU](&lt;&gt; agent_to_help==0)</formula>
            <comment/>
        </query>
        <query>
            <formula>Pr[&lt;=TAU](&lt;&gt; agent_to_help==1)</formula>
            <comment/>
        </query>
        <query>
            <formula>Pr[&lt;=TAU]([] agent_to_help==-1)</formula>
            <comment/>
        </query>
        <query>
            <formula>E[&lt;=TAU;1000](max: r_payoff)</formula>
            <comment/>
        </query>
        <query>
            <formula>strategy EndOptR = minE (r_payoff) [&lt;=TAU] : &lt;&gt; s.__end__</formula>
            <comment/>
        </query>
        <query>
