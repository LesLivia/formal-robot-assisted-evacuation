        int count_zr = 0;
        int count_fr = 0;

        broadcast chan help_victim, contact_first_responder;
        broadcast chan new;

        hybrid clock t;

        const int N_PI = 3;
        const int FR_SPEED = 1;
        // helper-gender helper-culture helper-age fallen-gender fallen-culture fallen-age
        const int PI_S[N_PI] = {0, 0, 0};
        const int D_V[N_V] = {4, 10, 17, 5, 9};
        const int D_FR[N_V] = {2, 20, 1, 18, 13};
        const int PI_V[N_V][N_PI] = {{0, 1, 0}, {2, 1, 0}, {2, 1, 0}, {1, 1, 3}, {3, 2, 1}};

        const int TAU = 30;
        hybrid clock evac_time = TAU;
    </declaration>
    <template>
        <name x="5" y="5">Survivor</name>
        <parameter>meta int id, meta int speed, meta bool rat_or_pi</parameter>
        <declaration>meta int dist = 0, dv, dfr;
            double r;

            void set_r(int i_v) {
            int i, count=0;
            for (i=0;i&lt;N_PI;i++)
            if(PI_S[i] == PI_V[i_v][i]) count++;
            if (rat_or_pi) r = 1.0;
            else r = count/N_PI;
            }

            void set(double r, double s, bool update_time, bool use_fr) {
            r_payoff = r_payoff + r;
            s_payoff = s_payoff + s;
            if (update_time) evac_time = t;
            if (use_fr) fr_usage = fr_usage + 1;
            else evac_time = 0;
            }
        </declaration>
